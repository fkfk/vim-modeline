{
  "name": "vim-modeline",
  "main": "./lib/vim-modeline",
  "version": "0.3.3",
  "description": "Enable Vim-style modeline in Atom.",
  "keywords": [
    "vim",
    "format",
    "code",
    "style",
    "code-style"
  ],
  "providedServices": {
    "vim-modeline-event-handler": {
      "description": "vim-modeline event handler",
      "versions": {
        "1.0.0": "provideVimModelineEventHandlerV1"
      }
    }
  },
  "configSchema": {
    "readLineNum": {
      "type": "integer",
      "default": 5,
      "minimum": 1
    },
    "prefix": {
      "type": "array",
      "default": [
        "vi",
        "vim",
        "ex",
        "atom"
      ],
      "items": {
        "type": "string"
      }
    },
    "insertPrefix": {
      "type": "string",
      "enum": [
        "vi",
        "vim",
        "ex",
        "atom"
      ],
      "default": "atom"
    },
    "insertModelinePosition": {
      "type": "string",
      "enum": [
        "first row",
        "last row",
        "above cursor row",
        "below cursor row"
      ],
      "default": "last row"
    },
    "insertModelineType": {
      "type": "string",
      "enum": [
        "short",
        "long",
        "original"
      ],
      "default": "long"
    }
  },
  "repository": "https://github.com/fkfk/vim-modeline",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0"
  },
  "dependencies": {
    "underscore-plus": "^1.6.6",
    "iconv-lite": "^0.4.13"
  }
}
